{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {
                "serverFiles": ["package.json"]
            }
        },
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/now-builder",
            "config": {
                "serverFiles": [".nuxt/dist/sitemap-routes.json"]
            }
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "continue": true,
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Service-Worker-Allowed": "/"
            }
        }
    ]
}
